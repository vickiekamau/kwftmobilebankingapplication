            android:name="com.example.administrator.kwft.activity.Confirm_Mpesa_Details.MobileMoney"
            android:label="Mpesa" />
        <activity android:name="com.example.administrator.kwft.activity.Confirm_Mpesa_Details.ConfirmDetails" />
        <activity
            android:name="com.example.administrator.kwft.activity.Balance.Balance"
            android:label="Balance" />
        <activity android:name="com.example.administrator.kwft.activity.Confirm_Mpesa_Details.SendMyNumber" />

        <provider
            android:name="com.android.tools.ir.server.InstantRunContentProvider"
            android:authorities="com.example.administrator.kwft.com.android.tools.ir.server.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>